{
    "name": "ttrpg-system-graph",
    "version": "0.0.1",
    "description": "A plugin development template for devs who want to use svelte in Obsidian.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "jest": {
        "preset": "ts-jest",
        "globals": {
            "ts-jest": {
                "isolatedModules": true
            }
        },
        "collectCoverage": true,
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ],
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "transform": {
            "^.+\\.(ts)$": "ts-jest"
        },
        "testMatch": [
            "**/*.test.ts",
            "**/*.test.js"
        ]
    },
    "scripts": {
        "style": "sass ./modules/styles.scss styles.css",
        "lint": "eslint . --ext .ts",
        "dev": "vite build --watch",
        "vite": "vite build --watch",
        "build": "npx tsc",
        "test": "jest --coverage"
    },
    "keywords": [],
    "author": "Grobax",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^16.18.87",
        "jest": "^29.7.0",
        "madge": "^8.0.0",
        "ts-jest": "^29.2.5",
        "tslib": "^2.6.2",
        "grobax-json-handler": "github:GrobaxGrappleBlast/GrobaxJSONHandler"
    }
}
